using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp9
{
    internal class Program
    {
        static void Main(string[] args)
        {
            string name;
            string surname;
            name ="Skobnikov";
            surname ="Andrey";
            Console.WriteLine($"{name} {surname}");
            name = "Andrey";
            surname = "Skobnikov";
            Console.WriteLine($"{name} {surname}");
        }
    }
}
