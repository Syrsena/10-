    internal class Program
    {
        static void Main(string[] args)
        {
            string surname = "Andrey";
            string name = "Skobnikov";
            string bufferSurname;
            string bufferName;
            Console.WriteLine($"Фамилия {surname} Имя {name}");

            buffername = surname;
            bufferSurname = name;

            name = bufferName;
            surname = bufferSurname;

            Console.WriteLine($"Фамилия {surname} Имя {name}");
        }
    }
